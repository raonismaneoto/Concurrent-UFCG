package sixth;

public class Consumer implements Runnable{
	
	@Override
	public void run() {
		while (true) {
			synchronized(this.chan) {
				while(this.chan.isEmpty()) {
					try {
						this.chan.wait();
					} catch (Exception e) {
					}
				}
				try {
					String message = this.chan.takeMessage();
					System.out.println("taken " + message);
				} catch (Exception e) {
				}
				this.chan.notifyAll();
			}
		}
	}

}
