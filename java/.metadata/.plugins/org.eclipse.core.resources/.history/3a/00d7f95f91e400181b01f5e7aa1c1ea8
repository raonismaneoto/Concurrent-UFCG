package first;

public class Main {
	
	class Producer implements Runnable {
		private Channel chan;
		
		public Producer(Channel chan) {
			this.chan = chan;
		}

		@Override
		public void run() {
			while(this.chan.isFull()) {
				this.chan.wait();
			}
			
		}
		
	}
	
	class Consumer implements Runnable{

		@Override
		public void run() {
			
			
		}
		
	}
	
	public static void main(String args[]) {
		
	}
}
