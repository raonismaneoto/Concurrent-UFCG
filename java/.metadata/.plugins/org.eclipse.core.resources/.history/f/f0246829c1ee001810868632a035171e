package second;

public class Main {
	private String data;
	
	private String reliableRequest() {
		Thread mirror1 = new Thread(new Mirror(this, "mirror1.com"));
		Thread mirror2 = new Thread(new Mirror(this, "mirror2.br"));
		Thread mirror3 = new Thread(new Mirror(this, "mirror3.edu"));
		
		
		mirror1.join();
		mirror2.join();
		mirror3.join();
		return "";
	}
	
	public static void main(String[] args) {
		Main m = new Main();
		String result = reliableRequest();
		System.out.println(result);
	}
}
